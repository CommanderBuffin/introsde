<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.docs.accounts.model.impl.AccountModelImpl">
		<attributes>
			<id name="accountId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="firstName" />
			<basic name="familyName" />
			<basic name="email" />
			<basic name="birthday" />
			<basic name="gender" />
			<basic name="accountPassword" />
			<basic name="maritalStatus" />
			<basic name="interests" />
			<basic name="educationLevel" />
			<basic name="foreignLanguages" />
			<basic name="profession" />
			<basic name="preferences" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.docs.accounts.model.impl.AccountImpl" name="Account">
		<table name="GB_Account" />
		<attributes />
	</entity>
</entity-mappings>
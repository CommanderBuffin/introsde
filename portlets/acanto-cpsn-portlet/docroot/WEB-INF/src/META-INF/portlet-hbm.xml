<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.docs.accounts.model.Account" />
	<class name="com.liferay.docs.accounts.model.impl.AccountImpl" table="GB_Account">
		<cache usage="read-write" />
		<id name="accountId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="firstName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="familyName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="birthday" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="gender" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="accountPassword" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="maritalStatus" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="interests" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="educationLevel" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="foreignLanguages" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="profession" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="preferences" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>